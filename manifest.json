{
  "name": "UDJ GrooveShark Player",
  "version" : "1.0",
  "manifest_version" : 2,
  "description" : "A UDJ Player that uses grooveshark as your music source.",
  "page_action":{
    "default_popup" : "popup.html",
    "default_title" : "Turn UDJ Player On/Off",
    "default_icon" : "icon-19.png"
  },
  "background":{
    "scripts" : ["grooveshark_page_watcher.js"]
  },
  "options_page" : "options.html",
  "icons" : {
    "48" : "icon-48.png",
    "128" : "icon-128.png"
  },
  "permissions" : [
    "tabs",
    "https://*.udjplayer.com/"
  ]
}
